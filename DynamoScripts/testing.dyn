= max(0, min(xmax, width - 1))\r\n    ymax = max(0, min(ymax, height - 1))\r\n\r\n    # Iterate over the bounding box region and change the color\r\n    for x in range(xmin, xmax + 1):\r\n        for y in range(ymin, ymax + 1):\r\n            pixels[x, y] = rgb_color\r\n            \r\n    \r\n    \r\n    return image , pixels\r\n    \r\ndef convert_to_view_geometry(bbox , x_min , y_min) : \r\n    reshaped_bbox = [*bbox[0] , *bbox[1]]\r\n    return [\r\n        dim + x_min if d % 2 == 0 else dim + y_min for d , dim in enumerate(reshaped_bbox)\r\n    ]\r\n\r\ndoc = DocumentManager.Instance.CurrentDBDocument\r\n\r\nviewId = IN[0]\r\nelementId = ElementId(viewId)\r\nview = doc.GetElement(elementId)\r\n#view = doc.ActiveView\r\noptions = ImageExportOptions()\r\noptions.ExportRange = ExportRange.CurrentView\r\noptions.ViewName = view.Name\r\n\r\ntemp_file = tempfile.mktemp(suffix=\".png\")\r\n\r\noptions.FilePath = temp_file\r\noptions.ZoomType = ZoomFitType.Zoom\r\noptions.PixelSize = 1024\r\n\r\ndoc.ExportImage(options)\r\n\r\n\r\n\r\norigins , destinations = IN[2]\r\n\r\nimage_path = f\"{temp_file[:-3]}jpg\"\r\nx0, y0, x1, y1 = *IN[1][0] , *IN[1][1]\r\nx_min , y_min , x_max , y_max = [i - j for i , j in zip(IN[3] , [x0 , y0 , x1 , y1])]\r\n\r\n\r\nimage = im.open(image_path)\r\npixels = image.load()\r\n\r\ncolor = (255, 0, 0)\r\nfor origin in origins : \r\n\r\n    xmin , ymin , xmax , ymax = convert_to_view_geometry(origin , x_min , y_min)\r\n    pixel_coordinates = transform_coordinates(image, x0, y0, x1, y1, xmin, ymin, xmax, ymax)\r\n    image , pixels = change_bounding_box_color(image , pixels, *pixel_coordinates, color)\r\n    \r\ncolor = (0, 255, 0)\r\nfor destination in destinations : \r\n\r\n    xmin , ymin , xmax , ymax = convert_to_view_geometry(destination , x_min , y_min)\r\n    pixel_coordinates = transform_coordinates(image, x0, y0, x1, y1, xmin, ymin, xmax, ymax)\r\n    image , pixels = change_bounding_box_color(image , pixels, *pixel_coordinates, color)\r\n    \r\ncolored_temp = tempfile.mktemp(suffix=\".png\")\r\nimage.save(colored_temp)\r\n\r\n\r\nout_img = Image.FromFile(colored_temp)\r\n\r\nOUT = x_min , y_min , x_max , y_max",
